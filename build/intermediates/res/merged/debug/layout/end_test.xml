<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/all_page_background"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/title_headr_size"
        android:background="@color/title_back_color"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/txt_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:paddingRight="40dp"
            android:text="Practice Tests"
            android:textColor="@color/title_text_color"
            android:textSize="@dimen/title_text_size" />

        <Button
            android:id="@+id/btn_logout"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/icon_logout" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="visible" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_your_result"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Your Results"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/txt_your_result"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Failed"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_category"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Category"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_category"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Law And Bussiness"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_testname"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Test Name"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_testname"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="C10 Test Name"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_totalque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Total Questions"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_totalque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="120"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_attemptedque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Attempted Questions"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_attemptedque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="12"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_rightque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="No. Of Correct Questions"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_rightque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="10"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_wrongque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="No. Of Incorrect Questions"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_wrongque"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="2"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="@dimen/margintop_end_test_row"
                android:orientation="horizontal"
                android:weightSum="2" >

                <TextView
                    android:id="@+id/txt_title_percentage"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Percentage"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />

                <TextView
                    android:id="@+id/txt_percentage"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="3%"
                    android:textColor="@color/edittext_text_color"
                    android:textSize="@dimen/end_test_text_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginTop="50dp"
                android:orientation="vertical"
                android:weightSum="1" >

                <TextView
                    android:id="@+id/txt_back_dashboard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="0.2"
                    android:background="@drawable/textlines"
                    android:singleLine="true"
                    android:text="Back to Dashboard"
                    android:textColor="@color/title_back_view"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:weightSum="0.5" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="0.2"
                    android:singleLine="true"
                    android:text="    "
                    android:textColor="@color/title_back_view"
                    android:textSize="16sp" />
            </LinearLayout>

            <!-- <LinearLayout -->
            <!-- android:layout_width="fill_parent" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:layout_centerInParent="true" -->
            <!-- android:layout_marginTop="35dp" -->
            <!-- android:orientation="horizontal" > -->


            <!-- <TextView -->
            <!-- android:id="@+id/txt_back_dashboard" -->
            <!-- android:layout_width="wrap_content" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:layout_marginBottom="10dp" -->
            <!-- android:background="@drawable/textlines" -->
            <!-- android:paddingTop="15dp" -->
            <!-- android:singleLine="true" -->
            <!-- android:text="Back to Dashboard" -->
            <!-- android:textColor="@color/title_back_view" -->
            <!-- android:textSize="20sp" /> -->
            <!-- </LinearLayout> -->
        </LinearLayout>
    </ScrollView>

    <!-- <LinearLayout -->
    <!-- android:layout_width="fill_parent" -->
    <!-- android:layout_height="wrap_content" -->
    <!-- android:layout_gravity="center" -->
    <!-- android:layout_marginTop="10dp" -->
    <!-- android:gravity="bottom" -->
    <!-- android:orientation="vertical" -->
    <!-- android:visibility="visible" > -->


    <!-- <TextView -->
    <!-- android:id="@+id/txt_back_dashboard" -->
    <!-- android:layout_width="fill_parent" -->
    <!-- android:layout_height="wrap_content" -->
    <!-- android:layout_gravity="center" -->
    <!-- android:layout_marginBottom="15dp" -->
    <!-- android:background="@drawable/textlines" -->
